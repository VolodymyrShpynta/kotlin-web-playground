# Web Playground Environment Configuration
# Copy this file to .env and fill in the values

# Application Environment (local, prod)
WEB_PLAYGROUND_ENV=prod

# HTTP Server Port (inside container)
WEB_PLAYGROUND_HTTP_PORT=4207

# Database Configuration
WEB_PLAYGROUND_DB_USER=
WEB_PLAYGROUND_DB_PASSWORD=
WEB_PLAYGROUND_DB_URL=jdbc:h2:./build/prod;MODE=PostgreSQL;DATABASE_TO_LOWER=TRUE;

# Security Settings
WEB_PLAYGROUND_USE_SECURE_COOKIE=false
WEB_PLAYGROUND_COOKIE_ENCRYPTION_KEY=1d13f63b868ad26c46151245e1b5175c
WEB_PLAYGROUND_COOKIE_SIGNING_KEY=d232897cbcc6cc89579bfbfc060632945e0dc519927c891733421f0f4a9ae48f

# For production with external database (PostgreSQL example):
# WEB_PLAYGROUND_DB_USER=postgres
